    "@types/react-dom": "^19.0.0",
    "expect": "^29.7.0",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
