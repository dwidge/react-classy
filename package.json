{
  "name": "@dwidge/react-classy",
  "version": "0.1.1",
  "description": "Lite styled components for React",
  "keywords": [],
  "author": "DWJ",
  "license": "BSL-1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwidge/react-classy.git"
  },
  "exports": {
    "./*": "./lib/*",
    ".": "./lib/index.tsx"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "servey src=src/index.tsx env=REACT_APP_ dir=public port=3000",
